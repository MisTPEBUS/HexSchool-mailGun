    "/v1/api/auth/sign_up": {
      "post": {
        "tags": [
          "使用者登入驗證"
        ],
        "summary": "會員註冊",
        "description": "會員註冊",
        "responses": {
          "default": {
            "description": ""
          }
        },
        "requestBody": {
          "required": true,
          "description": "會員資料",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string",
                    "example": "Lobinda"
                  },
                  "email": {
                    "type": "string",
                    "example": "Lobinda123@test.com"
                  },
                  "photo": {
                    "type": "string",
                    "example": ""
                  },
                  "password": {
                    "type": "string",
                    "description": "至少要8碼",
                    "example": "1q2w3e4r"
                  },
                  "phone": {
                    "type": "string",
                    "description": "可Null",
                    "example": "0987654321"
                  },
                  "address": {
                    "type": "string",
                    "description": "地址",
                    "example": "地球某個角落"
                  },
                  "date_of_birth": {
                    "type": "string",
                    "format": "date",
                    "description": "生日",
                    "example": "2006-08-18"
                  }
                },
                "required": [
                  "name",
                  "email",
                  "password"
                ]
              }
            }
          }
        }
      }
    },
