    "/v1/api/admin/account/": {
      "get": {
        "tags": [
          "會員管理"
        ],
        "summary": "會員清單",
        "description": "會員清單",
        "parameters": [
          {
            "name": "role",
            "in": "query",
            "description": "管理身分, user,admin",
            "enum": [
              "user",
              "admin",
              "undefined"
            ],
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "isBlackListed",
            "in": "query",
            "description": "1:搜尋黑名單 0:不是黑名單, 預設空直為搜尋全部",
            "enum": [
              1,
              0,
              "undefined"
            ],
            "schema": {
              "type": "boolean"
            }
          },
          {
            "name": "keyWord",
            "in": "query",
            "description": "關鍵字fuzzy[email,phone,address,dateOfBirth], 預設空直為搜尋全部",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "timeSort",
            "in": "query",
            "description": "排序遠到近,進到遠default = desc",
            "enum": [
              "asc",
              "desc"
            ],
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "limit",
            "in": "query",
            "description": "清單顯示比數,default=10",
            "schema": {
              "type": "number"
            }
          },
          {
            "name": "page",
            "in": "query",
            "description": "顯示第幾頁資料default=1",
            "schema": {
              "type": "number"
            }
          }
        ],
        "responses": {
          "default": {
            "description": ""
          }
        }
      }
    },
